<?xml version="1.0" encoding="utf-8"?>
<plugin
    xmlns="http://www.phonegap.com/ns/plugins/1.0"
    id="de.rwthaachen.rz.rwthapp.plugins.wp8language"
    version="0.1.0">

  <name>German Language Plugin for Windows Phone 8</name>
  
  <platform name="wp8">
    <config-file target="Properties/WMAppManifest.xml" parent="/Deployment/Languages">
        <Language code="de-DE"/>
        <Language code="en-US"/>
    </config-file>
  </platform>

</plugin>
